program testprogram {

	var variable1, variable2, temp1, temp2, temp3, result;

	function subprog1 (in tempparam) {

		var var1, var2, var3;

		var1 := 1;
		var2 := 2;
		var3 := 3;

		print (var1+var2+var3);

	}

	call subprog1;

	variable1 := 1;
	variable2 := 2;
	temp1 := 192;
	temp2 := 241;
	temp3 := 124;

	result := temp1 + temp2 + temp3 - variable1 - variable2;

	print (result);

}